{
  "name": "portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start-server": "ts-node --project server/tsconfig.server.json server/index.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh hooks/pre-commit"
    }
  },
  "dependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@react-pdf-viewer/core": "^3.6.0",
    "@react-pdf-viewer/thumbnail": "^3.6.0",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "next": "12.1.6",
    "pdfjs-dist": "^2.14.305",
    "prettier": "^2.7.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-pdf-thumbnail": "^0.1.0",
    "react-router-dom": "^6.3.0",
    "sass": "^1.52.2",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/next": "^9.0.0",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.6",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "husky": "^8.0.0"
  }
}
